{
    "name": "Orthography Extension",
    "version": "1.0",
    "manifest_version": 3,
    "description": "An extension to convert words to orthography.",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [ "content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["mappings.json"],
        "matches": ["*://*/*"]
      }
    ]
  }
  